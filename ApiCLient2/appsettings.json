{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "DefaultScheme": "Bearer",
  "DefaultChallengeScheme": "oidc",
  "IdentityServerAuthenticationOptions": {
    "Authority": "https://localhost:44314/",
    "RequireHttpsMetadata": true,
    "ApiName": "apiClient",
    "ApiSecret": "secret",
    "ResponseType": "code id_token",
    "SaveTokens": true,
    "GetClaimsFromUserInfoEndpoint": true
  }
}
